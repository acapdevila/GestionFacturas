@model GestionFacturas.Modelos.FiltroBusquedaFactura

@using (Html.BeginForm(null, null, FormMethod.Get, new { @class = "form-inline" }))
{
    <div class="form-group">
        @Html.TextBoxFor(m => m.NombreOEmpresaCliente, new { @class = "form-control", placeholder = "Nombre cliente" })
    </div>
    <div class="form-group">
        @Html.TextBoxFor(m => m.FechaDesde, new { @class = "form-control", placeholder = "Desde" })
    </div>
    <div class="form-group">
        @Html.TextBoxFor(m => m.FechaHasta, new { @class = "form-control", placeholder = "Hasta" })
    </div>
    <button type="submit" class="btn btn-default">Buscar</button>
}



      
